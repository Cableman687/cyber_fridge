<h1 class="text-center login-title">CyberFridge!</h1>

<h2>Sign Up</h2>

<form class="form signup-form">
  <div class="form-group">
    <label for="name-signup">Name:</label>
    <input class="form-control" type="text" id="name-signup" />
  </div>
  <div class="form-group">
    <label for="email-signup">Email:</label>
    <input class="form-control" type="text" id="email-signup" />
  </div>
  <div class="form-group">
    <label for="password-signup">Password:</label>
    <input class="form-control" type="password" id="password-signup" />
  </div>
  <div class="form-group col-md-4">
    <button class="btn btn-primary" type="button">Sign Up!</button>
  </div>
  <div class="form-group col-md-4">
    <button class="btn btn-primary" type="button" id="login-button">Go Back!</button>
  </div>
</form>



<script src="/js/signup.js"></script>
<script src="/js/transfers/transferToLogin.js"></script>
